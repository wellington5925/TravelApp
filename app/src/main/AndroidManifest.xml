<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.senacsp.travelapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Inicial_Activity" />
        <activity android:name=".SingUp_Activity" />
        <activity android:name=".SignInActivity" />
        <activity
            android:name=".ProfileActivity"
            android:theme="@style/Actionbar" />
        <activity
            android:name=".HomeActivity"
            android:theme="@style/Actionbar" />
        <activity android:name=".PlaneTickettActivity" />
        <activity android:name=".resultsActivity" />
        <activity android:name=".FliResultsActivity"></activity>
    </application>

</manifest>